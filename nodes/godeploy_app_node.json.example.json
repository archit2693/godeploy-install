{
  "run_list": ["recipe[godeploy-cookbook::install]"],
  	"app_name": "godeploy",
	  "command_name": "bundle exec rails server -e production -b localhost",
	  "script_location": "/home/godeploy/deploy_start.sh",
    "environment_variables": {
    		"DB_NAME": "godeploy",
    		"RAILS_ENV": "production",
			  "CAS_LOGIN_URL": "https://www.example.com/cas/login",
			  "DB_USER": "deploy",
			  "DB_HOST": "localhost",
			  "DB_PORT": "5432",
			  "DB_PASSWORD": "password",
			  "DB_POOL": "10",
			  "DB_TIMEOUT": "5000",
			  "ENABLE_CAS_INTEGRATION": "false",
			  "CAS_BASE_URL": "https://www.example.com/cas",
			  "SMTP_ADDRESS": "smtp.gmail.com",
			  "SMTP_PORT": "587",
			  "SMTP_DOMAIN": "domain.com",
			  "SMTP_USER_NAME": "username@domain.com",
			  "SMTP_PASSWORD": "password",
			  "DIFF_BASE_URL": "http://localhost:3000",
			  "DOMAIN_BASE_URL": "http://localhost:3000",
			  "GITLAB_BASE_URL": "https://www.gitlab.com/",
			  "JIRA_BASE_URL": "https://example.atlassian.net/",
			  "JIRA_PROJECT_KEY": "DEMO",
			  "JIRA_ISSUE_TYPE": "Task",
			  "SLACK_WEBHOOK": "https://hooks.slack.com/services/hash/hash/key",
			  "GROUP_NOTIFICATION_MAIL": "example-group@mail.com"
			}
}